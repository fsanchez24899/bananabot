{
  "name": "peckbot",
  "version": "0.0.0",
  "private": true,
  "author": "Jack Serrino <jserrino@gmail.com>",
  "description": "The slackbot that powers the PKT slack",
  "scripts": {
    "start-redis": "redis-server > /dev/null & echo $! > redis.pid",
    "stop-redis": "kill -TERM $(cat redis.pid) && rm redis.pid",
    "start": "node_modules/hubot/bin/hubot -l peckbot"
  },
  "dependencies": {
    "chrono-node": "^1.0.8",
    "google-auth-library": "^0.9.6",
    "googleapis": "^2.1.5",
    "hubot": "^2.12.0",
    "hubot-conf": "^1.0.0",
    "hubot-diagnostics": "0.0.1",
    "hubot-group": "^1.0.0",
    "hubot-help": "^0.1.1",
    "hubot-principles": "^1.0.0",
    "hubot-redis-brain": "0.0.2",
    "hubot-seen": "^0.2.2",
    "hubot-shortcut": "^1.0.0",
    "hubot-slack": "^3.3.0",
    "moment": "^2.10.6",
    "request": "^2.69.0",
    "timeago": "^0.2.0",
    "xero": "0.0.8"
  },
  "engines": {
    "node": "0.10.x"
  }
}
