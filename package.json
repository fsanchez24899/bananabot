{
  "name": "peckbot",
  "version": "0.0.0",
  "private": true,
  "author": "Jack Serrino <jserrino@gmail.com>",
  "description": "The slackbot that powers the PKT slack",
  "scripts": {
    "start-redis": "redis-server > /dev/null & echo $! > redis.pid",
    "stop-redis": "kill -TERM $(cat redis.pid) && rm redis.pid",
    "start": "bin/hubot -a slack"
  },
  "dependencies": {
    "hubot": "^2.12.0",
    "hubot-conf": "^1.0.0",
    "hubot-diagnostics": "0.0.1",
    "hubot-group": "^1.0.0",
    "hubot-help": "^0.1.1",
    "hubot-redis-brain": "0.0.2",
    "hubot-seen": "^0.2.2",
    "hubot-shortcut": "^1.0.0",
    "hubot-slack": "^3.3.0",
    "timeago": "^0.2.0"
  },
  "engines": {
    "node": "0.10.x"
  }
}
